{
  "name": "snow-launch",
  "version": "2.0.0",
  "description": "",
  "main": "./dist/main.js",
  "scripts": {
    "build:main": "cross-env NODE_ENV=production webpack --config webpack/webpack.main.prod.config.js",
    "build:renderer": "cross-env NODE_ENV=production webpack --config webpack/webpack.renderer.prod.config.js",
    "build:worker": "cross-env NODE_ENV=production webpack --config webpack/webpack.worker.prod.config.js",
    "build:prepare": "npm run build:main && npm run build:renderer && npm run build:worker",
    "build:electron": "npm run build:prepare && electron-builder --ia32",
    "dev:renderer": "webpack-dev-server --config webpack/webpack.renderer.dev.config.js",
    "dev:worker": "webpack-dev-server --config webpack/webpack.worker.dev.config.js",
    "dev:main": "webpack --config webpack/webpack.main.config.js && electron ./dist/main.js",
    "start-dev": "./start.sh",
    "prestart": "npm run build:prepare",
    "start": "electron .",
    "lint": "eslint --ext=jsx,js,tsx,ts src",
    "pretest:e2e": "npm run build",
    "pack": "npm run build && electron-builder --dir --x64 --ia32",
    "postinstall": "electron-builder install-app-deps"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,test,mocks}/**/*.{json,css,scss,md}": [
      "prettier --config ./.prettierrc --write"
    ],
    "{src,test,mocks}/**/*.{js,ts,tsx}": [
      "prettier --config ./.prettierrc --write",
      "eslint --ext=jsx,js,ts,tsx --fix src"
    ]
  },
  "build": {
    "productName": "Snowlaunch",
    "appId": "org.snowbot.snowlaunch",
    "mac": {
      "category": "your.app.category.type"
    },
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "linux": {
      "target": "deb"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "requestedExecutionLevel": "highestAvailable"
    },
    "nsis": {
      "createDesktopShortcut": "always",
      "artifactName": "${productName} Setup.${ext}",
      "differentialPackage": true,
      "oneClick": false,
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true
    },
    "portable": {
      "artifactName": "Snowlaunch.exe"
    }
  },
  "author": {
    "name": "-",
    "email": "-@-.com"
  },
  "license": "-",
  "bugs": {
    "url": "http://"
  },
  "homepage": "",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@hot-loader/react-dom": "^16.8.6",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/react": "^16.8.18",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.0.9",
    "@types/react-test-renderer": "^16.8.1",
    "@types/webdriverio": "^4.8.7",
    "@types/webpack-env": "^1.13.3",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.1.3",
    "css-loader": "^2.1.1",
    "electron": "^3.1.9",
    "electron-builder": "^22.3.2",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^1.3.4",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.1",
    "image-webpack-loader": "^4.6.0",
    "lint-staged": "^10.0.7",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "react-hot-loader": "^4.8.8",
    "react-test-renderer": "^16.8.6",
    "redux-devtools-extension": "^2.13.5",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "spectron": "^5.0.0",
    "style-loader": "^0.23.1",
    "typescript": "^3.4.5",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "electron-log": "^4.3.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "redux": "^4.0.1"
  }
}